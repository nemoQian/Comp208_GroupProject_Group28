<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

    <mapper namespace = "com.group28.dao.UserDao">
        <select id = "getUserList" resultType = "com.group28.pojo.User">
            select * from user_information.user;
        </select>

        <select id = "getUser" resultType="com.group28.pojo.User" parameterType="String">
            select * from user_information.user where user_Name = #{user_Name};
        </select>
    </mapper>

